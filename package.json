    "devDependencies": {
        "@types/jquery": "^3.5.29",
        "copy-webpack-plugin": "^12.0.2",
        "eslint": "^8.39.0",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.8",
        "ts-loader": "^9.5.1",
        "typescript": "^5.4.5",
        "webpack": "^5.81.0",
        "webpack-cli": "^5.0.2"
    },
